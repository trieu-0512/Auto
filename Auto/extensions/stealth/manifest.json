{
  "manifest_version": 3,
  "name": "Stealth Patches",
  "version": "0.1.0",
  "description": "Hide common automation fingerprints.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
