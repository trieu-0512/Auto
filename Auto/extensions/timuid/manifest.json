{
    "name": "TimUid.com",
    "description": "TimUid.com",
    "version": "1.3",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "*://timuid.com/*","*://*.facebook.com/*",
        "webRequest","webRequestBlocking"
    ],
    "externally_connectable": {
        "matches": ["*://timuid.com/*","*://www.facebook.com/*","*://m.facebook.com/*","*://mbasic.facebook.com/*"]
    },
    "browser_action": {
        "default_title": "TimUid.com",
        "default_icon": "icon_128.png"
    },
    "background": {
        "scripts": [ "background.js" ],
        "all_frames": true
    }, 
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}